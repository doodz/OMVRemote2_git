<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestUi.TestActivity">

    <LinearLayout
        android:id="@+id/LinearLayoutTest"
        android:orientation="vertical"
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">
        <Button
            android:id="@+id/SharesDeserializerBtn"
            android:text="Test SharesDeserializer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Button
            android:id="@+id/SharedFolderNFSBtn"
            android:text="Test SharesDeserializer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



        <Button
            android:id="@+id/LongParsingBtn"
            android:text="Test LongParsing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.google.android.gms.ads.AdView
            android:id="@+id/banner1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:adSize="BANNER"
            app:adUnitId="ca-app-pub-4922361220283829/4711179794"
            />
        <com.google.android.gms.ads.NativeExpressAdView
            android:id="@+id/banner2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:adSize="360x132"
            app:adUnitId="ca-app-pub-4922361220283829/7664646197"
            />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
